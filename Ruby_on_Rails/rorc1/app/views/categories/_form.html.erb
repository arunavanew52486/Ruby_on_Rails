<%= form_with(model: @category) do |form| %>
  <div class="mb-3">
    <%= form.label "Category name", style: "display: block", class:"form-label" %>
    <%= form.text_field :name, class:"form-control" %>
  </div>

  <div class="mb-3">
    <%= form.submit class:"btn btn-primary" %>
  </div>
<% end %>
